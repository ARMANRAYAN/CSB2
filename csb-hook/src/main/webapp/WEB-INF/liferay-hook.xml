<?xml version="1.0"?>
<!DOCTYPE hook PUBLIC "-//Liferay//DTD Hook 6.1.0//EN" "http://www.liferay.com/dtd/liferay-hook_6_1_0.dtd">

<hook>
    <portal-properties>portal.properties</portal-properties>
    <custom-jsp-dir>/META-INF/custom_jsps</custom-jsp-dir>
    <service>
        <service-type>com.liferay.portlet.blogs.service.BlogsEntryLocalService</service-type>
        <service-impl>com.armansoft.csb.service.wrapper.BlogsEntryLocalServiceWrapperImpl</service-impl>
    </service>
    <struts-action>
        <struts-action-path>/portal/imageUploader</struts-action-path>
        <struts-action-impl>no.uninett.agora.ckeditor.ImageUploaderStrutsAction</struts-action-impl>
    </struts-action>
    <struts-action>
        <struts-action-path>/portal/getpageattachment</struts-action-path>
        <struts-action-impl>no.uninett.agora.ckeditor.GetPageAttachmentStrutsAction</struts-action-impl>
    </struts-action>

</hook>